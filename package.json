{
  "name": "browscap-js-cache",
  "description": "the cache class for browscap.js",
  "keywords": [
    "browscap"
  ],
  "author": "Thomas Müller",
  "contributors": [
    "Thomas Müller (http://github.com/mimmi20)"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/mimmi20/browscap-js-cache.git"
  },
  "bugs": {
    "url": "http://github.com/mimmi20/browscap-js-cache/issues"
  },
  "bin": {
    "browscap-cache": "./index.js"
  },
  "main": "src/index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/mimmi20/browscap-js/README.md"
    }
  ],
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-google": "^0.8.0",
    "lint-staged": "^4.3.0",
    "mocha": "^3.0.0",
    "nyc": "^11.7.3",
    "prettier": "^1.11.1"
  },
  "scripts": {
    "test": "nyc --clean --reporter=text mocha -u tdd --full-trace ./test/*.js",
    "test-and-coverage": "nyc --clean --reporter=clover mocha -u tdd --full-trace ./test/*.js",
    "precommit": "lint-staged"
  },
  "homepage": "https://github.com/mimmi20/browscap-js",
  "version": "1.2.0",
  "directories": {
    "test": "test"
  },
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "prettier --print-width 117 --single-quote --write",
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 1,
    "statements": 1,
    "functions": 1,
    "branches": 1,
    "cache": false,
    "all": true
  }
}
